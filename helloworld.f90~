program hello_world
implicit none

! Deklarationsteil
integer :: wert

! Ende des Deklarationsteil

print *,'Hallo Welt' ! * ist ein Formatparameter, kann explizit angegeben werden
print *,'Test\n'
print *,'Bitte eine Eingabe machen!'
print *
read *, wert
print *,'wert = ',wert
end program hello_world
